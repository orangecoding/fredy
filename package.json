{
  "name": "fredy",
  "version": "11.0.3",
  "description": "[F]ind [R]eal [E]states [d]amn eas[y].",
  "scripts": {
    "start": "bun prod.ts",
    "dev": "bun install && bun run clean-ui && vite",
    "ui": "bun run clean-ui && vite",
    "prod": "bun install && vite build --emptyOutDir",
    "format": "prettier --write lib/**/*.ts ui/src/**/*.tsx ui/src/**/*.ts test/**/*.ts *.ts --single-quote --print-width 120",
    "test": "mocha --loader=esmock --timeout 3000000 test/**/*.test.ts",
    "lint": "eslint --ext .ts,.tsx ./index.ts ./lib/ ./test/ ./ui/src/",
    "clean-ui": "rimraf ui\\public\\* --glob"
  },
  "type": "module",
  "lint-staged": {
    "*.ts": [
      "eslint ./index.ts ./lib/**/*.ts ./test/**/*.ts",
      "prettier --write"
    ]
  },
  "main": "index.ts",
  "author": "Christian Kellner",
  "keywords": [
    "flat",
    "flatfinder",
    "fredy",
    "real estates",
    "germany",
    "apartment",
    "house",
    "rent",
    "immoscout",
    "scraper",
    "immonet",
    "immowelt",
    "immobilienscout24"
  ],
  "bugs": {
    "url": "https://github.com/orangecoding/fredy/issues"
  },
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0",
    "npm": ">=7.0.0"
  },
  "browserslist": [
    "> 0.5%",
    "not dead",
    "not ie 6-11",
    "Firefox ESR"
  ],
  "imports": {
    "#types/*": "./lib/types/*",
    "#services/*": "./lib/services/*",
    "#ui_services/*": "./ui/src/services/*",
    "#provider/*": "./lib/provider/*"
  },
  "dependencies": {
    "@douyinfe/semi-ui": "2.73.0",
    "@rematch/core": "^2.2.0",
    "@rematch/loading": "^2.1.2",
    "@sendgrid/mail": "8.1.4",
    "@types/better-sqlite3": "^7.6.13",
    "@types/body-parser": "^1.19.6",
    "@types/chai": "^5.2.2",
    "@types/cookie-session": "^2.0.49",
    "@types/lodash": "^4.17.19",
    "@types/mocha": "^10.0.10",
    "@types/node": "^22.15.34",
    "@types/react-router-dom": "^5.3.3",
    "@types/redux-logger": "^3.0.13",
    "@types/serve-static": "^1.15.8",
    "@types/string-similarity": "^4.0.2",
    "@vitejs/plugin-react": "4.3.4",
    "better-sqlite3": "^11.10.0",
    "body-parser": "1.20.3",
    "cheerio": "^1.1.0",
    "cookie-session": "^2.1.0",
    "handlebars": "4.7.8",
    "highcharts": "12.1.2",
    "highcharts-react-official": "3.2.1",
    "lodash": "4.17.21",
    "lowdb": "6.0.1",
    "markdown": "^0.5.0",
    "mixpanel": "^0.18.1",
    "nanoid": "5.0.9",
    "node-fetch": "3.3.2",
    "node-mailjet": "6.0.6",
    "package-up": "^5.0.0",
    "puppeteer": "^24.11.1",
    "puppeteer-extra": "^3.3.6",
    "puppeteer-extra-plugin-stealth": "^2.11.2",
    "query-string": "9.1.1",
    "react": "18.3.1",
    "react-dom": "18.3.1",
    "react-redux": "9.2.0",
    "react-router": "5.2.1",
    "react-router-dom": "5.3.0",
    "redux": "5.0.1",
    "redux-thunk": "3.1.0",
    "restana": "4.9.9",
    "serve-static": "1.16.2",
    "slack": "11.0.2",
    "string-similarity": "^4.0.4",
    "ts-migrate": "^0.1.35",
    "vite": "5.4.11"
  },
  "devDependencies": {
    "@babel/core": "7.26.0",
    "@babel/eslint-parser": "7.26.5",
    "@babel/preset-env": "7.26.0",
    "@babel/preset-react": "7.26.3",
    "@eslint/eslintrc": "^3.3.1",
    "@eslint/js": "^9.30.0",
    "@typescript-eslint/eslint-plugin": "^8.35.0",
    "@typescript-eslint/parser": "^8.35.0",
    "chai": "5.1.2",
    "eslint": "^9.30.0",
    "eslint-config-prettier": "^10.1.5",
    "eslint-plugin-prettier": "^5.5.1",
    "eslint-plugin-react": "^7.37.5",
    "esmock": "2.6.9",
    "globals": "^16.2.0",
    "history": "5.3.0",
    "husky": "9.1.7",
    "less": "4.2.1",
    "lint-staged": "15.4.1",
    "mocha": "10.8.2",
    "prettier": "^3.6.2",
    "redux-logger": "3.0.6",
    "rimraf": "^6.0.1",
    "typescript": "^5.8.3",
    "typescript-eslint": "^8.35.0"
  }
}
